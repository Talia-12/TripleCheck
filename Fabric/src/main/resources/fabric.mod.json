{
  "schemaVersion": 1,
  "id": "yourmod",
  "version": "${version}",

  "name": "Your Mod",
  "description": "A blank mod template",
  "authors": [
    "petrak@ (aka gamma-delta)",
    "grywo (just turning her work into a template)"
  ],
  "contact": {
    "homepage": "",
    "sources": ""
  },

  "license": "MIT",
  "icon": "logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {"adapter": "kotlin", "value": "yourmod.name.here.fabric.FabricInitializer"}
    ],
    "client": [
      {"adapter": "kotlin", "value": "yourmod.name.here.fabric.FabricClientInitializer"}
    ],
    "fabric-datagen": [
      "yourmod.name.here.datagen.FabricDataGenerators"
    ]
  },
  "mixins": [
    "yourmod.plat.mixins.json",
    "yourmod.mixins.json"
  ],
  "accessWidener": "yourmod.accesswidener",

  "depends": {
    "fabricloader": ">=0.13",
    "fabric": ">=0.50",
    "minecraft": "=1.18.2",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21"
  },
  "suggests": {
  },

  "custom": {
    "cardinal-components": [
    ]
  }
}
